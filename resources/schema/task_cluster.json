{
	"settings": {
		"index.number_of_replicas": 1,
		"index.number_of_shards": 5
	},
	"mappings": {
		"task": {
			"_source": {
				"compress": true
			},
			"_id" : {
				"index": "not_analyzed",
				"type": "string",
				"store": true
	        },
			"_all": {
				"enabled": false
			},
			"dynamic_templates": [
				{
					"default_ids": {
						"mapping": {
							"index": "not_analyzed",
							"type": "string",
							"doc_values": true
						},
						"match": "id"
					}
				},
				{
					"default_strings": {
						"mapping": {
							"index": "not_analyzed",
							"type": "string",
							"doc_values": true
						},
						"match_mapping_type": "string",
						"match": "*"
					}
				},
				{
					"default_doubles": {
						"mapping": {
							"index": "not_analyzed",
							"type": "double",
							"doc_values": true
						},
						"match_mapping_type": "double",
						"match": "*"
					}
				},
				{
					"default_longs": {
						"mapping": {
							"index": "not_analyzed",
							"type": "long",
							"doc_values": true
						},
						"match_mapping_type": "long|integer",
						"match_pattern": "regex",
						"path_match": ".*"
					}
				}
			],
			"properties": {
				"task":{
					"type": "object",
					"dynamic": true,
					"properties": {
						"artifacts": {
							"type": "nested"
						},
						"tags": {
							"type": "nested"
						},
						"cache": {
							"type": "nested"
						},
						"image": {
							"type": "object",
							"dynamic": true,
							"properties": {
								"cache": {
									"type": "nested"
								}
							}
						}
					}
				},
				"etl":{
					"type":"object",
					"properties":{
						"id":{
							"type":"integer",
							"doc_values": true
						},
						"source":{
							"type":"object",
							"properties":{
								"id":{
									"type":"integer",
									"doc_values": true
								},
								"source":{
									"type":"object",
									"properties":{
										"id":{
											"type":"integer",
											"doc_values": true
										},
										"source":{
											"type":"object",
											"properties":{
												"id":{
													"type":"integer",
													"doc_values": true
												}
											}
										}
									}
								}

							}
						}

					}

				}
			}
		}
	}

}
