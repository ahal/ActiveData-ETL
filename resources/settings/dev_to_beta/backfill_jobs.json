{
	"limit": 100,
	"no_checks": true,
	"range": {"min": 0},
	"source": {
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-jobs-beta",
		"key_format": "a:b.c"
	},
	"destination": {
		"host": "http://192.168.2.98",
		"port": 9200,
		"index": "jobs",
		"type": "job",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/jobs.json"
		},
		"debug": false,
		"limit_replicas": false
	},
	"work_queue":{
		"name": "active-data-index-jobs-beta",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		}
	},
	"debug": {
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV]Problem in backfil jobs",
				"$ref": "file://~/private.json#email"

			}
		]
	}
}
