{
	"work_queue": {
		"name": "active-data-index-es5",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"workers": [
		{
			"name": "firefox-files",
			"batch_size": 1000,
			"skip": 0,
			"rollover":{
				"field": "etl.source.source.timestamp",
				"interval": "week",
				"max": "3week"
			},
			"sample_only": [],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "firefox-files",
				"type": "files",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",
				"schema": {
					"$ref": "//../../schema/files.json"
				},
				"debug": false,
				"limit_replicas": false
			},
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-firefox-files",
				"key_format": "t.a:b.c.d"
			},
			"backfill": {
				"prime_key": "etl.source.source.source.id",
				"source_key": "etl.source.source.source.source.source.code"
			}
		},
		{
			"name": "fx_test",
			"batch_size": 1000,
			"skip": 0,
			"rollover":{
				"field": "result.start_time",
				"interval": "3month",
				"max": "2year"
			},
			"sample_only": [
			],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "fx-test",
				"type": "test_result",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/fx_test.json"
				},
				"debug": false,
				"limit_replicas": true
			},
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-fx-test-normalized",
				"key_format": "a.b"
			}
		},
		{
			"name": "treeherder",
			"batch_size": 1000,
			"skip": 0,
			"rollover":{
				"field": "action.start_time",
				"interval": "3month",
				"max": "2year"
			},
			"sample_only": [
			],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "treeherder",
				"type": "th_job",
				"id": "job.id",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/treeherder.json"
				},
				"debug": false,
				"limit_replicas": true,
				"_id": "job.id"
			},
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-treeherder-normalized",
				"key_format": "a.b"
			}
		},
		{
			"name": "unittest",
			"batch_size": 1000,
			"skip": 0,
			"rollover":{
				"field": {"coalesce":["build.date","repo.push.date", "run.timestamp"]},
				"interval": "week",
				"max": "5week"
			},
			"sample_only": [
				"try"
			],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "unittest",
				"type": "test_result",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/test_results.json"
				},
				"debug": true,
				"limit_replicas": false
			},
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-test-result",
				"key_format": "t.a:b.c"
			},
			"backfill":{
				"prime_key": "etl.source.source.id",
				"source_key": "etl.source.source.source.source.code"
			}
		},
		{
			"name": "perf",
			"rollover":{
				"field": {"coalesce":["build.date","repo.push.date", "run.timestamp"]},
				"interval": "year",
				"max":"2year"
			},
			"batch_size": 1000,
			"queue_size": 2000,
			"sample_only": ["try"],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "perf",
				"type": "perf_result",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/perf_results.json"
				},
				"debug": false,
				"limit_replicas": false
			},
			"source": {
				"bucket": "active-data-perf",
				"$ref": "file://~/private.json#aws_credentials",
				"key_format": "t.a:b.c"
			},
			"backfill":{
				"ignore_key": "etl.source.id",
				"prime_key": "etl.source.source.source.id",
				"source_key": "etl.source.source.source.source.source.code"
			}
		},
		{
			"name": "jobs",
			"rollover": {
				"field": "build.date",
				"interval": "3month",
				"max":"2year"
			},
			"batch_size": 500,
			"sample_only": [],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "jobs",
				"type": "job",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/jobs.json"
				},
				"debug": false,
				"limit_replicas": false
			},
			"source": {
				"bucket": "active-data-jobs",
				"$ref": "file://~/private.json#aws_credentials",
				"key_format": "a.b"
			},
			"backfill":{
				"prime_key": "etl.source.source.id", // "etl.source.id"],
				"source": null,
				"source_key": null
			}
		},
		{
			"name": "code coverage",
			"skip": 0.0,
			"queue_size": 1000,
			"batch_size": 500,
			"rollover": {
				"field": "repo.push.date",
				"interval": "week",
				"max": "2week"
			},
			"sample_only": [],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "coverage",
				"type": "code_coverage",
				"tjson": true,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"timeout": 300,
				"schema": {
					"$ref": "//../../schema/code_coverage.json"
				},
				"debug": true,
				"limit_replicas": false
			},
			"source": {
				"bucket": "active-data-codecoverage",
				"$ref": "file://~/private.json#aws_credentials",
				"key_format": "t.a:b.c.d"
			}
		},
		{
			"name": "task",
			"queue_size": 2000,
			"batch_size": 1000,
			"rollover": {
				"field": "task.created",
				"interval": "3month",
				"max": "2year"
			},
			"sample_only": [],
			"elasticsearch": {
				"host": "http://localhost",
				"port": 9200,
				"index": "task",
				"type": "task",
				"id": "task.id",
				"tjson": true,
				"timeout": 300,
				"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
				"schema": {
					"$ref": "//../../schema/task_cluster.json"
				},
				"debug": false,
				"limit_replicas": false
			},
			"source": {
				"bucket": "active-data-task-cluster-normalized",
				"$ref": "file://~/private.json#aws_credentials",
				"key_format": "t.a:b"
			}
		}
	],
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		}
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "console"
			},
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "logs/push_to_es_direct.log",
				"maxBytes" : 10000000,
				"backupCount" : 10,
				"encoding" : "utf8"
			},
			{
				"log_type": "ses",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][Staging6]Problem in Push to ES",
				"$ref": "file://~/private.json#aws_credentials"
			}
		]
	}
}
