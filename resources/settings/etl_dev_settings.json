		{
			"name": "pulse2es",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger-dev"
			},
			"destination": {
				"use_daily": false,
				"host": "http://localhost",
				"port": 9200,
				"index": "pulse",
				"type": "pulse_log",
				"timeout": 300,
				"schema": {
					"$ref": "//../schema/pulse_logs.json"
				},
				"debug": true,
				"limit_replicas": true
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_es.process_pulse_block_to_es",
			"type": "join"
		},
		{
			"name": "pulse2talos",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger-dev"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-talos-dev"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_talos_logs.process_talos",
			"type": "join"
		},
		{
			"name": "pulse2test_result",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger-dev"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-test-result-dev"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_test_result_logs.process_talos",
			"type": "join"
		}
