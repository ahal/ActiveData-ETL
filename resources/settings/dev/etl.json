{
	"work_queue": {
		"name": "active-data-etl-dev",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"param": {
		"threads": 1,
		"keep_unknown_on_queue": false,
		"wait_forever": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"testlog_etl.transforms.pulse_block_to_test_result_logs.PARSE_TRY": true
	},
	"hg":{
		"use_cache": true,
		"hg":{
			"url": "https://hg.mozilla.org"
		},
		"branches":{
			"host": "http://localhost",
			"port": 9200,
			"index": "branches",
			"type": "branch",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/branch.json"
			},
			"debug": false,
			"limit_replicas": true
		},
		"repo":{
			"host": "http://localhost",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/revision.json"
			},
			"debug": false,
			"limit_replicas": true
		}
	},
