		{
			"name": "tc_pulse to TC Normalized",
			"source": {
				"bucket": "active-data-task-cluster-logger-dev",
				"public": true,
				"key_format": "t.a:b",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"destination": {
				"bucket": "active-data-task-cluster-normalized-dev",
				"public": true,
				"key_format": "t.a:b",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"notify": {
			   "name": "active-data-etl-dev",
			   "$ref": "file://~/private.json#aws_credentials"
		   	},
			"transformer": "testlog_etl.transforms.pulse_block_to_task_cluster.process",
			"type": "join"
		},
		{
			"name": "taskcluster to test_result",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-task-cluster-normalized-dev",
				"key_format": "t.a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-test-result-dev",
				"public": true,
				"key_format": "t.a:b.c"
			},
			"notify": {
				"name": "active-data-index-unit-dev",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "testlog_etl.transforms.task_cluster_to_test_result.process",
			"type": "join"
		},
