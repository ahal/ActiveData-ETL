{
	"force": true,
	"source": {
		"url": "http://builddata.pub.build.mozilla.org/builddata/buildjson/"
	},
	"destination": {
		"bucket": "active-data-buildbot-dev",
		"public": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"notify": {
		"name": "active-data-etl-dev",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"elasticsearch": {
		// ES USED TO DETERMINE WHAT IS MISSING
		"id_field": "etl.source.source.id",
		"host": "http://activedata.allizom.org",
		"port": 9200,
		"index": "jobs",
		"type": "job",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/jobs.json"
		},
		"debug": true,
		"limit_replicas": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		}
	},
	"debug": {
		"trace": true,
		"cprofile": false,
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV] Problem in Buildbot Parsing",
				"$ref": "file://~/private.json#email"
			}
		]
	}
}
