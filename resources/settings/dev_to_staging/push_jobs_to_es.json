{
	"threads": 1,
	"batch_size": 500,
	"sample_only": [],
	"elasticsearch": {
		"host": "http://activedata.allizom.org",
		"port": 9200,
		"index": "jobs",
		"type": "job",
		"timeout": 300,
		"schema": {
			"$ref": "//../../schema/jobs.json"
		},
		"debug": false,
		"limit_replicas": false
	},
	"source": {
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-jobs",
		"key_format": "a:b.c"
	},
	"work_queue": {
		"name": "active-data-index-jobs",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		}
	},
	"debug": {
		"trace":true,
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV]Problem in Push jobs to ES",
				"$ref": "file://~/private.json#email"

			}
		]
	}
}
