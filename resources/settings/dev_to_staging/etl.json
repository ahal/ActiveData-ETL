{
	"work_queue": {
		"name": "active-data-etl",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"param": {
		"threads": 1,
		"wait_forever": true,
		"keep_unknown_on_queue": true,
		"min_attempts": 0
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
//			"User-Agent": "kyle's dev machine debugging ActievData-ETL"
		},
		"pyLibrary.env.http.DEBUG": false,
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 10000000,
		"activedata_etl.transforms.pulse_block_to_test_result_logs.PARSE_TRY": true
	},
	"treeherder": {
		"use_cache": true,
		"rate_limiter": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "treeherder_requests",
			"type": "request",
			"timeout": 3,
			"schema": {
				"mappings": {
					"my_type": {
						"_ttl": {
							"enabled": true,
							"default": "10m"
						}
					}
				}
			},
			"debug": false,
			"limit_replicas": false
		},
		"cache": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "treeherder",
			"type": "job",
			"timeout": 300,
			"debug": false,
			"limit_replicas": false
		}
	},
	"hg": {
		"use_cache": true,
		"hg": {
			"url": "https://hg.mozilla.org"
		},
		"branches": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "branches",
			"type": "branch",
			"timeout": 300,
			"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
			"schema": {
				"$ref": "//../../schema/branch.json"
			},
			"debug": false,
			"limit_replicas": false
		},
		"repo": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
			"schema": {
				"$ref": "//../../schema/revision.json"
			},
			"debug": false,
			"limit_replicas": false
		}
	},
	"workers": [
		{
		"name": "Buildbot Pulse to ES",
		"source": {
			"$ref": "file://~/private.json#aws_credentials",
			"bucket": "active-data-pulse",
			"key_format": "t.a:b"
		},
		"destination": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "pulse",
			"type": "pulse_log",
			"timeout": 300,
			"consistency": "one",   // WE ONLY NEED ONE SHARD TO BE AVAILABLE
			"schema": {
				"$ref": "//../../schema/pulse_logs.json"
			},
			"debug": false,
			"limit_replicas": true
		},
		"transformer": "activedata_etl.transforms.pulse_block_to_es.process",
		"type": "join"
	},
	{
		"name": "tc_pulse to TC Normalized",
		"source": {
			"bucket": "active-data-task-cluster-logger",
			"public": true,
			"key_format": "t.a:b",
			"$ref": "file://~/private.json#aws_credentials"
		},
		"destination": {
			"bucket": "active-data-task-cluster-normalized",
			"public": true,
			"key_format": "t.a:b",
			"$ref": "file://~/private.json#aws_credentials"
		},
		"notify": [
			{
				"name": "active-data-etl",
				"$ref": "file://~/private.json#aws_credentials"
			},
			{
				"name": "active-data-index",
				"$ref": "file://~/private.json#aws_credentials"
			}
		],
		"transformer": "activedata_etl.transforms.pulse_block_to_task_cluster.process",
		"type": "join"
	},
	{
		"name": "TC Normalized to test_result",
		"source": {
			"$ref": "file://~/private.json#aws_credentials",
			"bucket": "active-data-task-cluster-normalized",
			"key_format": "t.a:b"
		},
		"destination": {
			"$ref": "file://~/private.json#aws_credentials",
			"bucket": "active-data-test-result",
			"public": true,
			"key_format": "t.a:b.c"
		},
		"notify": {
			"name": "active-data-index",
			"$ref": "file://~/private.json#aws_credentials"
		},
		"transformer": "activedata_etl.transforms.task_cluster_to_test_result.process",
		"type": "join"
	},
	{
		"name": "pulse2perfherder",
		"source": {
			"$ref": "file://~/private.json#aws_credentials",
			"bucket": "active-data-pulse",
			"public": true,
			"key_format": "t.a:b"
		},
		"destination": {
			"$ref": "file://~/private.json#aws_credentials",
			"bucket": "active-data-perfherder",
			"public": true,
			"key_format": "t.a:b.c"
		},
		"notify": {
			"name": "active-data-etl",
			"$ref": "file://~/private.json#aws_credentials"
		},
		"transformer": "activedata_etl.transforms.pulse_block_to_perfherder_logs.process",
		"type": "join",
		"transform_type": "bulk"
	},
		{
			"name": "bbb2jobs",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-buildbot",
				"key_format": "a.b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-jobs",
				"public": true,
				"key_format": "a.b"
			},
			"notify": {
				"name": "active-data-index",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "activedata_etl.transforms.buildbot_block_to_job_logs.process",
			"type": "join",
			"transform_type": "bulk"
		},
		{
			"name": "perfherder2perf",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-perfherder",
				"key_format": "t.a:b.c"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-perf",
				"public": true,
				"key_format": "t.a:b.c"
			},
			"notify": {
				"name": "active-data-index",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "activedata_etl.transforms.perfherder_logs_to_perf_logs.process",
			"type": "join"
		},
		{
			"name": "pulse2test_result",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-pulse",
				"key_format": "t.a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-test-result",
				"public": true,
				"key_format": "t.a:b.c"
			},
			"notify": {
				"name": "active-data-index",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "activedata_etl.transforms.pulse_block_to_test_result_logs.process",
			"type": "join"
		},
		{
			"name": "tc_normalized to codecoverage",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-task-cluster-normalized",
				"key_format": "t.a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-codecoverage",
				"public": true,
				"key_format": "t.a:b.c.d"
			},
			"notify": {
				"name": "active-data-index",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "activedata_etl.transforms.jscov_to_es.process",
			"type": "join"
		}
	],
	"debug": {
		"trace": true,
		"cprofile": {
			"enabled": false,
			"filename": "./results/profile.tab"
		},
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "elasticsearch",
				"host": "http://localhost",
				"index": "debug",
				"type": "etl",
				"retry": {"times": 3, "sleep": "minute"}
			},
			{
				"log_type": "ses",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV] Problem in ETL",
				"$ref": "file://~/private.json#aws_credentials"
			}
		]
	}
}
