{
	"work_queue": {
		"name": "ekyle-etl",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"param": {
		"threads": 1,
		"wait_forever": true,
		"keep_unknown_on_queue": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 10000000,
		"testlog_etl.transforms.pulse_block_to_test_result_logs.INDEX_TRY": false
	},
	"workers": [
		{
			"name": "pulse2es",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger",
				"key_format": "a:b"
			},
			"destination": {
				"use_daily": false,
				"host": "http://54.149.35.214",
				"port": 9200,
				"index": "pulse",
				"type": "pulse_log",
				"timeout": 300,
				"schema": {
					"$ref": "//../schema/pulse_logs.json"
				},
				"debug": true,
				"limit_replicas": true
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_es.process_pulse_block_to_es",
			"type": "join"
		},
		{
			"name": "pulse2talos",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger",
				"key_format": "a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-talos",
				"public": true,
				"key_format": "a:b"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_talos_logs.process",
			"type": "join",
			"transform_type": "bulk"
		},
		{
			"name": "pulse2test_result",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-pulse-logger",
				"key_format": "a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "ekyle-test-result",
				"key_format": "a:b.c"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_test_result_logs.process",
			"type": "join"
		}
	],
	"debug": {
		"trace": true,
		"cprofile": {
			"enabled": false,
			"filename": "./results/profile.tab"
		},
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "elasticsearch",
				"host": "http://54.149.35.214",
				"index": "debug",
				"type": "unittest_etl"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV] Problem in ETL",
				"$ref": "file://~/private.json#email"
			}

		]
	}
}
